    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.1",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.4",
    "graphql-upload": "^11.0.0",
    "helmet": "^3.23.3",
    "inquirer": "^7.3.2",
    "jsonwebtoken": "^8.5.1",
    "limiter": "^1.1.5",
    "moment": "^2.27.0",
