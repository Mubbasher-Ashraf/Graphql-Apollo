{
  "name": "graphql_apollo-project",
  "version": "1.0.0",
  "private": "true",
  "main": "src/index.js",
  "description": "React with Apollo and Node with Graphql + mongoDB",
  "scripts": {
    "dev": "nodemon --watch .env --watch . --exec babel-node src",
    "client": "npm start --prefix client",
